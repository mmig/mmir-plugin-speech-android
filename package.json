{
  "name": "mmir-plugin-speech-android",
  "version": "1.0.0",
  "description": "Cordova plugin for the Android speech recognizer and synthesizer",
  "cordova": {
    "id": "mmir-plugin-speech-android",
    "platforms": [
      "android"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmig/mmir-plugin-speech-android.git"
  },
  "keywords": [
    "cordova",
    "speech",
    "android",
    "asr",
    "speech recognition",
    "tts",
    "text to speech",
    "ecosystem:cordova",
    "cordova-android"
  ],
  "engines": {
    "cordovaDependencies": {}
  },
  "peerDependencies": {
    "mmir-lib": ">=5"
  },
  "devDependencies": {
    "mmir-plugin-exports": "^1.4.2",
    "mmir-lib": "^6.0.0"
  },
  "main": "./www/asrAndroid.js",
  "directories": {
    "lib": "./www"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "createplugincompat ./ && pluginexport ./",
    "install-scripts": "copycordovascripts res/js"
  },
  "mmir": {
    "exports": [
      "./www/ttsAndroid.js"
    ],
    "compat": {
      "./www/asrAndroid.js": {
        "file": "./www/alt/asrAndroidCompat.js",
        "type": "media"
      },
      "./www/ttsAndroid.js": {
        "file": "./www/alt/ttsAndroidCompat.js",
        "type": "media"
      }
    }
  },
  "author": "russa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmig/mmir-plugin-speech-android/issues"
  },
  "homepage": "https://github.com/mmig/mmir-plugin-speech-android#readme"
}
